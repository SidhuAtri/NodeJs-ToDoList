<%-include("header")%>

<div class="box" id="heading1">
  <h1><%= weekday %></h1>
</div>

<div class="box" id="heading2">
  <h2><%= general %></h2>
</div>

<div class="box" id="">
  <% for(var i =0; i < newListItems.length; i++){ %>
  <div class="item">
    <input type="checkbox" />
    <p><%=newListItems[i]%></p>
  </div>
  <% } %>
  <form class="item" action="/" method="POST">
    <input
      type="text"
      name="newItem"
      placeholder="New Item"
      autocomplete="off"
    />
    <button type="submit" name="submit">+</button>
  </form>
</div>

<%-include("footer")%>
